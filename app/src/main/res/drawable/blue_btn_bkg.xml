<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/palette_orange"/>
            <stroke
                android:color="@color/palette_orange2"
                android:width="1dp"/>
        </shape>
    </item>


    <item>
        <shape>

            <solid

                android:color="@color/palette_cyan2"/>
            <stroke
                android:color="@color/palette_cyan2"
                android:width="1dp"/>
            <corners
                android:radius="50dp"
                />
        </shape>
    </item>

</selector>