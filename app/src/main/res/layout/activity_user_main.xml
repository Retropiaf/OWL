<?xml version="1.0" encoding="utf-8"?>
<android.widget.LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.owl.UserMainActivity">


    <Button
        android:id="@+id/sleep_circle_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sleep_circles_btn"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="285dp" />


    <Button
        android:id="@+id/sleep_session"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sleep_session"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="209dp" />


    <Button
        android:id="@+id/new_sleep_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/new_sleep_session"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="447dp" />

</android.widget.LinearLayout>
